<picture>
  {% if picture.srcDesktopWide %}
  <source media="{{ desktopWide }}" srcset="{{ picture.srcDesktopWide }}">
  {% endif %}
  {% if picture.srcDesktopLarge %}
  <source media="{{ desktopLarge }}" srcset="{{ picture.srcDesktopLarge }}">
  {% endif %}
  {% if picture.srcDesktop %}
  <source media="{{ desktop }}" srcset="{{ picture.srcDesktop }}">
  {% endif %}
  {% if picture.srcTablet %}
  <source media="{{ tablet }}" srcset="{{ picture.srcTablet }}">
  {% endif %}
  {% if picture.src %}
  <img src="{{ picture.src }}" alt="{{ picture.alt }}">
  {% endif %}
</picture>
